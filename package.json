{
  "name": "adityadharacom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "gcp-predeploy": "find ./build -name '\\[...*' -exec bash -c 'mv \"$1\" \"${1/.../@@@}\"' -- {} \\;",
    "gcp-build": "npm ci --production && find ./build -name '\\[@@@*' -exec bash -c 'mv \"$1\" \"${1/@@@/...}\"' -- {} \\;",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "start": "next start -p 8080",
    "deploy": "npm run build && npm run gcp-predeploy && gcloud app deploy"
  },
  "dependencies": {
    "next": "^10.0.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.10.1",
    "@types/react": "^16.9.49",
    "typescript": "^4.0.2"
  }
}
